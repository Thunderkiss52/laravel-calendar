{
    "name": "Thunderkiss52/laravel-calendar",
    "description": "Laravel calendar",
    "require": {
        "php": ">=8.2",
        "spatie/laravel-model-info": "^2.0",
        "laravel/framework": "^11.0",
        "spatie/laravel-data": "^4.8",
        "spatie/laravel-package-tools": "^1.18",
        "Thunderkiss52/laravel-eloquent-authorable": "dev-master",
        "Thunderkiss52/laravel-forms": "dev-main"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.org"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Thunderkiss52/laravel-eloquent-authorable.git"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Thunderkiss52\\LaravelCalendar\\CalendarProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Thunderkiss52\\LaravelCalendar\\": "src/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "baseline": "vendor/bin/phpstan analyse --generate-baseline",
        "test": "vendor/bin/pest"
    },
    "authors": [
        {
            "name": "Thunderkiss52",
            "email": "Thunderkiss52@gmail.com"
        }
    ]
}
